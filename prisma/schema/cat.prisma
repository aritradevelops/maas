model Cat {
  id          String      @id @default(cuid())
  name        String
  breed       String
  image       String
  personality Personality
  owner_id    String
  created_at  DateTime    @default(now())
  updated_at  DateTime    @updatedAt
}

enum Personality {
  Playful
  Calm
  Bold
  Affectionate
  Energetic
  Gentle
  Curious
  Confident
  Friendly
  Mysterious
}
